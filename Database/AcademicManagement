	[FirstHalfDayMinHours] [int] NULL,
	[SecondHalfDayTotalHours] [int] NULL,
	[SecondHalfDayMinHours] [int] NULL,
	[IsDeleted] [int] NULL,
	[CreatedDate] [datetime] NULL,
	[ModifiedDate] [datetime] NULL,
	[CollegeID] [int] NOT NULL,
